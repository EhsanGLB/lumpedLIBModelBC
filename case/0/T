FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 300;
    }

    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            zeroGradient;
    }

    LIB1
    {

        type            lumpedLIBModelBC;
        state           discharge;
        rho             2964;
        Cp              1108;
        I               3.2;
        R               0.2;
        dEdT            4.0e-4;
        Ti              300.0;
        SOC             96;//(%)
        C               3.2;//(Ah)
        V               1.57e-5;
        kf              0.026;
/*
        type            fixedValue;
        value           uniform 350;*/
    }

    LIB2
    {
        type            fixedValue;
        value           uniform 350;
    }

    LIB3
    {
        type            fixedValue;
        value           uniform 350;
    }
}


